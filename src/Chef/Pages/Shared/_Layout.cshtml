<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Chef</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="false" />
    @*<link rel="stylesheet" href="~/Chef.styles.css" asp-append-version="true" />*@
</head>
<body>
    <header>
        <div>
            <a class="navbar-brand" asp-area="" asp-page="/Index">Chef</a>
            <div class="navbar">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-page="/Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-page="/Privacy">Privacy</a>
                    </li>
                </ul>
            </div>
        </div>
    </header>
    <div>
        <main role="main" >
            @RenderBody()
        </main>
    </div>

    <footer>
        <div>
   
        </div>
    </footer>

    <script src="~/js/site.js" asp-append-version="false"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>